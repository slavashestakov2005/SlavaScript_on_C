class Point{
	map = 0
	arr = 0
	func = 0
	def Point(map, arr, func){
		this.map = map
		this.arr = arr
		this.func = func
	}
}

a = new Point({"two" : 2}, [0, 1, [2, 3]], def(x) = -x)
b = new Point({2 : "two"}, [[3, 2], 1, 0], def(x) = x)

println "Объекты сконструированы: " + a.map
println b
b.map = a.map
println b.map
